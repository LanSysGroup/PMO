<Application   
  x:Class="LanSysWebGIS.App"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:app="clr-namespace:LanSysWebGIS"
  Startup="Application_Startup"
  UnhandledException="Application_UnhandledException"
  Exit="Application_Exit">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/Styles.xaml"/>
                <ResourceDictionary Source="Resources/SmoothExpanderStyle.xaml"/>
                <ResourceDictionary Source="Resources/HRDExpanderStyle.xaml"/>
                <ResourceDictionary Source="Resources/GeoMapsStyle.xaml"/>
                <ResourceDictionary Source="Resources/Converters.xaml"/>
                <ResourceDictionary Source="Resources/HnrStyles.xaml"/>
                <ResourceDictionary Source="Assets/FlatStyles.xaml"/>
                <ResourceDictionary>
                    <app:ApplicationResources x:Key="ApplicationResources" />
                    <app:NotOperatorValueConverter x:Key="NotOperatorValueConverter" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>                
    </Application.Resources>

</Application>